    "test:integration": "npm run build && node --test dist-tests/integration/*.test.js",
    "test:all": "npm run build && node --test dist-tests/unit/*.test.js dist-tests/integration/*.test.js",
    "connectivity": "node --enable-source-maps dist/cli.js test"
  },
  "dependencies": {
    "@xdevplatform/xdk": "^0.4.0"
