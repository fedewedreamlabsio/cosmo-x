  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cosmo-x": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.test.json",
    "build:src": "tsc -p tsconfig.json",
    "dev": "tsc --watch",
    "cli": "node --enable-source-maps dist/cli.js",
